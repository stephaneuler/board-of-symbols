    <codeA>
        <![CDATA[// show all forms in a loop

int sleepTime = 1000;
groesse(1, 1);
hintergrund(0, WHITE);
farbe(0, GREEN);

for (SymbolType type : SymbolType.values()) {
  form(0, SymbolType.getShort(type));
  statusText("Form: "  + SymbolType.getShort(type) + " " + type);
  Thread.sleep(sleepTime);
}


